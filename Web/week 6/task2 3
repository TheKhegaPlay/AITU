/**
 * Welcome to the Stanford Karel IDE.
 * This is a free space for you to 
 * write any Karel program you want.
 **/
function main(){
   move();
   putBeeper();
   move();
   move();
   putBeeper();
   move();
   
   putBeepers2();
   
   putBeepers1();
   
   putBeepers2();
   
   putBeepers1();
}

function putBeepers1(){
   ifLeft();
   move();
   putBeeper();
   move();
   move();
   putBeeper();
   move();
}

function putBeepers2(){
   ifRight();
   putBeeper();
   move();
   move();
   putBeeper();
   move();
   move();
   putBeeper();
}

function ifRight(){
   turnLeft();
   move();
   turnLeft();
   
}

function ifLeft(){
   turnRight();
   move();
   turnRight();
}

